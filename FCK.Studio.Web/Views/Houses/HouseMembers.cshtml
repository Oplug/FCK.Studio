@model List<FCK.Studio.Web.Dto.MemberOutDto>
@{
    ViewBag.Title = "HouseMembers";
    Layout = "~/Views/Shared/_LayoutPageInner.cshtml";
}
<section class="content">
    <div class="card">
        <!-- /.card-header -->
        <div class="card-body table-responsive p-0">
            <table class="table table-hover">
                <tbody>
                    <tr>
                        <th>姓名</th>
                        <th>关系</th>
                        <th>性别</th>
                        <th>年龄</th>
                        <th>身份证号</th>
                        <th>户籍地址</th>
                    </tr>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>@(item.TrueName)</td>
                            <td>@(item.Relations)</td>
                            <td>@(item.Sex)</td>
                            <td>@(item.Age)</td>
                            <td>@(item.UserID)</td>
                            <td>@(item.Address )</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
        <!-- /.card-body -->
    </div>
</section>

