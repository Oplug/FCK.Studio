<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>@(ViewBag.Title)</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <link rel="stylesheet" href="/Content/bower_components/jqueryconfirm/jquery-confirm.css" />
    <!-- toastr -->
    <link rel="stylesheet" href="/Content/bower_components/toast/toastr.css" />
    <!-- Theme style -->
    <link rel="stylesheet" href="/Content/dist/css/adminlte.min.css">
    <!-- Google Font: Source Sans Pro -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
    <style>
        .img-responsive {
            display: block;
            height: auto;
            max-width: 100%;
        }
    </style>
    @RenderSection("css", required: false)
</head>
<body class="hold-transition sidebar-mini">
    <div class="wrapper">
        <nav class="main-header navbar navbar-expand bg-white navbar-light border-bottom">
            <!-- Left navbar links -->
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#"><i class="fa fa-bars"></i></a>
                </li>
                <li class="nav-item d-none d-sm-inline-block">
                    <a href="/Home/" class="nav-link">首页</a>
                </li>
                <li class="nav-item d-none d-sm-inline-block">
                    <a href="/Home/Contact" class="nav-link">联系我们</a>
                </li>
            </ul>
            <!-- SEARCH FORM -->
            <form class="form-inline ml-3">
                <div class="input-group input-group-sm">
                    <input class="form-control form-control-navbar" type="search" placeholder="产权证书办理" aria-label="Search">
                    <div class="input-group-append">
                        <button class="btn btn-navbar" type="submit">
                            <i class="fa fa-search"></i>
                        </button>
                    </div>
                </div>
            </form>
            <ul class="navbar-nav ml-auto">
                <li class="nav-item">
                    <a class="nav-link" href="/Home/">
                        <i class="fa fa-home"></i>
                    </a>
                </li>
            </ul>
        </nav>
        <aside class="main-sidebar sidebar-dark-primary elevation-4">
            <!-- Sidebar -->
            <div class="sidebar">
                <!-- Sidebar Menu -->
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                        <li class="nav-item has-treeview">
                            <a href="/Home/" class="nav-link">
                                <i class="nav-icon fa fa-home"></i>
                                <p>
                                    首页
                                </p>
                            </a>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="#" class="nav-link">
                                <i class="nav-icon fa fa-tree"></i>
                                <p>
                                    兴塘社区
                                    <i class="fa fa-angle-left right"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="/Home/About" class="nav-link">
                                        <i class="fa fa-circle-o nav-icon"></i>
                                        <p>社区简介</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="/Home/About?tit=人员公示" class="nav-link">
                                        <i class="fa fa-circle-o nav-icon"></i>
                                        <p>人员公示</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="/Home/Map" class="nav-link">
                                        <i class="fa fa-circle-o nav-icon"></i>
                                        <p>平面图</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="/Home/About?tit=网格管理" class="nav-link">
                                        <i class="fa fa-circle-o nav-icon"></i>
                                        <p>网格管理</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="/Activity/?cate=通知公告" class="nav-link">
                                        <i class="fa fa-circle-o nav-icon"></i>
                                        <p>通知公告</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="/Home/Service" class="nav-link">
                                        <i class="fa fa-circle-o nav-icon"></i>
                                        <p>办事流程</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="/Home/Login" class="nav-link">
                                        <i class="fa fa-circle-o nav-icon"></i>
                                        <p>用户登录</p>
                                    </a>
                                </li>
                            </ul>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="#" class="nav-link">
                                <i class="nav-icon fa fa-table"></i>
                                <p>
                                    党建公益
                                    <i class="fa fa-angle-left right"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="/Home/PartBuild" class="nav-link">
                                        <i class="fa fa-circle-o nav-icon"></i>
                                        <p>社区党建</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="/Activity/?cate=活动报名" class="nav-link">
                                        <i class="fa fa-circle-o nav-icon"></i>
                                        <p>活动报名</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="/Activity/?cate=党员风采" class="nav-link">
                                        <i class="fa fa-circle-o nav-icon"></i>
                                        <p>党员风采</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="/Home/Association" class="nav-link">
                                        <i class="fa fa-circle-o nav-icon"></i>
                                        <p>社团组织</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="/Home/PublicWelfare" class="nav-link">
                                        <i class="fa fa-circle-o nav-icon"></i>
                                        <p>公益组织</p>
                                    </a>
                                </li>
                            </ul>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="#" class="nav-link">
                                <i class="nav-icon fa fa-book"></i>
                                <p>
                                    便民服务
                                    <i class="fa fa-angle-left right"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="/Home/Feedback" class="nav-link">
                                        <i class="fa fa-circle-o nav-icon"></i>
                                        <p>访客心声</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="/DemdInfo/" class="nav-link">
                                        <i class="fa fa-circle-o nav-icon"></i>
                                        <p>邻里互助</p>
                                    </a>
                                </li>
                            </ul>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="/Member/Login" class="nav-link">
                                <i class="nav-icon fa fa-users"></i>
                                <p>
                                    用户中心
                                    <i class="fa fa-angle-left right"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="/Member/MyProfile" class="nav-link">
                                        <i class="fa fa-circle-o nav-icon"></i>
                                        <p>户籍录改</p>
