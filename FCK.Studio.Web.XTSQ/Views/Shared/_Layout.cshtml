                                <li class="nav-item">
                                    <a href="/Home/Election" class="nav-link">
                                        <i class="fa fa-circle-o nav-icon"></i>
                                        <p>选举投票</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="/Home/About?tit=信用管理" class="nav-link">
                                        <i class="fa fa-circle-o nav-icon"></i>
                                        <p>信用管理</p>
                                    </a>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </nav>
                <!-- /.sidebar-menu -->
            </div>
            <!-- /.sidebar -->
        </aside>
        <div class="content-wrapper">
            @RenderBody()
        </div>
        <footer class="main-footer">
            <small class="float-right d-none d-sm-block">
                <b>Version</b> 3.0.0-alpha
            </small>
            <small>
                Copyright &copy; @(DateTime.Now.Year) <a href="http://www.Oplug.cn">Oplug.cn</a>. All rights
                reserved.
            </small>
        </footer>
    </div>
    <script src="/Content/plugins/jquery/jquery.min.js"></script>
    <script src="/Content/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="/Content/bower_components/toast/toastr.min.js"></script>
    <script src="/Scripts/vue.min.js"></script>
    <script src="/Content/dist/js/adminlte.min.js"></script>
    <script src="/Content/dist/js/demo.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
